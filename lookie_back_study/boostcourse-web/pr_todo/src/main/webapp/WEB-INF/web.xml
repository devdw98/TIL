<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>MainServlet</servlet-name>
  	<display-name>MainServlet</display-name>
  	<description></description>
  	<servlet-class>dw.study.lookie.pr_todo.servlet.MainServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>TodoFormServlet</servlet-name>
  	<display-name>TodoFormServlet</display-name>
  	<description></description>
  	<servlet-class>dw.study.lookie.pr_todo.servlet.TodoFormServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>TodoAddServlet</servlet-name>
  	<display-name>TodoAddServlet</display-name>
  	<description></description>
  	<servlet-class>dw.study.lookie.pr_todo.servlet.TodoAddServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>TodoTypeServlet</servlet-name>
  	<display-name>TodoTypeServlet</display-name>
  	<description></description>
  	<servlet-class>dw.study.lookie.pr_todo.servlet.TodoTypeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>MainServlet</servlet-name>
  	<url-pattern>/main</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>TodoFormServlet</servlet-name>
  	<url-pattern>/todoForm</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>TodoAddServlet</servlet-name>
  	<url-pattern>/todoAdd</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>TodoTypeServlet</servlet-name>
  	<url-pattern>/todoType</url-pattern>
  </servlet-mapping>
</web-app>
